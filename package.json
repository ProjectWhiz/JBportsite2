{
  "name": "portfolio-website",
  "version": "1.0.0",
  "description": "Portfolio website with React and Flask",
  "scripts": {
    "start:all": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && C:/jbportsite/.venv/Scripts/python.exe app.py",
    "build": "cd frontend && npm run build",
    "build:start": "npm run build && npm run start:backend",
    "install:all": "npm install && cd frontend && npm install",
    "test:frontend": "cd frontend && npm test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "portfolio",
    "react",
    "flask"
  ],
  "author": "",
  "license": "MIT"
}
